    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@tarojs/cli": "^4.0.0",
    "@tarojs/taro-loader": "^4.0.0",
    "@tarojs/webpack5-runner": "^4.0.0",
    "@types/webpack-env": "^1.18.0",
    "@types/react": "^18.0.0",
    "typescript": "^5.1.0",
    "webpack": "^5.78.0",
    "@babel/core": "^7.8.0",
